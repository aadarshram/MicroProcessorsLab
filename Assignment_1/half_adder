#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x560ffa8d70a0 .scope module, "tb_half_adder" "tb_half_adder" 2 2;
 .timescale 0 0;
v0x560ffa8e9cb0_0 .var "a", 0 0;
v0x560ffa8e9d70_0 .var "b", 0 0;
v0x560ffa8e9e40_0 .net "c", 0 0, L_0x560ffa8ea170;  1 drivers
v0x560ffa8e9f40_0 .net "s", 0 0, L_0x560ffa8ea010;  1 drivers
S_0x560ffa8d7230 .scope module, "h1" "half_adder" 2 5, 3 2 0, S_0x560ffa8d70a0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "s";
    .port_info 3 /OUTPUT 1 "c";
L_0x560ffa8ea010 .functor XOR 1, v0x560ffa8e9cb0_0, v0x560ffa8e9d70_0, C4<0>, C4<0>;
L_0x560ffa8ea170 .functor AND 1, v0x560ffa8e9cb0_0, v0x560ffa8e9d70_0, C4<1>, C4<1>;
v0x560ffa89dcf0_0 .net "a", 0 0, v0x560ffa8e9cb0_0;  1 drivers
v0x560ffa8e9a10_0 .net "b", 0 0, v0x560ffa8e9d70_0;  1 drivers
v0x560ffa8e9ad0_0 .net "c", 0 0, L_0x560ffa8ea170;  alias, 1 drivers
v0x560ffa8e9b70_0 .net "s", 0 0, L_0x560ffa8ea010;  alias, 1 drivers
    .scope S_0x560ffa8d70a0;
T_0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560ffa8e9cb0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560ffa8e9d70_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560ffa8e9cb0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560ffa8e9d70_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560ffa8e9cb0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560ffa8e9d70_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560ffa8e9cb0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560ffa8e9d70_0, 0, 1;
    %delay 30, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x560ffa8d70a0;
T_1 ;
    %vpi_call 2 21 "$monitor", $time, " a = %b, b = %b, c = %b, s = %b ", v0x560ffa8e9cb0_0, v0x560ffa8e9d70_0, v0x560ffa8e9e40_0, v0x560ffa8e9f40_0 {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_half_adder.v";
    "half_adder.v";
