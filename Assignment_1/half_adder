:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55b7582d60a0 .scope module, "tb_half_adder" "tb_half_adder" 2 2;
 .timescale 0 0;
v0x55b7582e8e00_0 .var "a", 0 0;
v0x55b7582e8ec0_0 .var "b", 0 0;
v0x55b7582e8f90_0 .net "c", 0 0, L_0x55b7582e92c0;  1 drivers
v0x55b7582e9090_0 .net "s", 0 0, L_0x55b7582e9160;  1 drivers
S_0x55b7582d6230 .scope module, "h1" "half_adder" 2 5, 3 2 0, S_0x55b7582d60a0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "s";
    .port_info 3 /OUTPUT 1 "c";
L_0x55b7582e9160 .functor XOR 1, v0x55b7582e8e00_0, v0x55b7582e8ec0_0, C4<0>, C4<0>;
L_0x55b7582e92c0 .functor AND 1, v0x55b7582e8e00_0, v0x55b7582e8ec0_0, C4<1>, C4<1>;
v0x55b75829ccf0_0 .net "a", 0 0, v0x55b7582e8e00_0;  1 drivers
v0x55b7582e8b60_0 .net "b", 0 0, v0x55b7582e8ec0_0;  1 drivers
v0x55b7582e8c20_0 .net "c", 0 0, L_0x55b7582e92c0;  alias, 1 drivers
v0x55b7582e8cc0_0 .net "s", 0 0, L_0x55b7582e9160;  alias, 1 drivers
    .scope S_0x55b7582d60a0;
T_0 ;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b7582e8e00_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b7582e8ec0_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b7582e8e00_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b7582e8ec0_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b7582e8e00_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55b7582e8ec0_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b7582e8e00_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55b7582e8ec0_0, 0, 1;
    %delay 30, 0;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_0;
    .scope S_0x55b7582d60a0;
T_1 ;
    %vpi_call 2 22 "$monitor", $time, " a = %b, b = %b, s = %b, c = %b ", v0x55b7582e8e00_0, v0x55b7582e8ec0_0, v0x55b7582e9090_0, v0x55b7582e8f90_0 {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
