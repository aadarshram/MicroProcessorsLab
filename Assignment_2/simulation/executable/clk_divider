#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55ecb1a83930 .scope module, "tb_clk_divider" "tb_clk_divider" 2 3;
 .timescale -9 -12;
v0x55ecb1a96280_0 .var "inClk", 0 0;
v0x55ecb1a96350_0 .net "outClk", 0 0, v0x55ecb1a960a0_0;  1 drivers
v0x55ecb1a96420_0 .var "reset", 0 0;
S_0x55ecb1a83ac0 .scope module, "cd" "clk_divider" 2 6, 3 2 0, S_0x55ecb1a83930;
 .timescale 0 0;
    .port_info 0 /OUTPUT 1 "outClk";
    .port_info 1 /INPUT 1 "inClk";
    .port_info 2 /INPUT 1 "reset";
v0x55ecb1a4aba0_0 .var "clockCount", 25 0;
v0x55ecb1a95fe0_0 .net "inClk", 0 0, v0x55ecb1a96280_0;  1 drivers
v0x55ecb1a960a0_0 .var "outClk", 0 0;
v0x55ecb1a96140_0 .net "reset", 0 0, v0x55ecb1a96420_0;  1 drivers
E_0x55ecb1a49780/0 .event negedge, v0x55ecb1a96140_0;
E_0x55ecb1a49780/1 .event posedge, v0x55ecb1a95fe0_0;
E_0x55ecb1a49780 .event/or E_0x55ecb1a49780/0, E_0x55ecb1a49780/1;
    .scope S_0x55ecb1a83ac0;
T_0 ;
    %wait E_0x55ecb1a49780;
    %load/vec4 v0x55ecb1a96140_0;
    %cmpi/e 0, 0, 1;
    %jmp/0xz  T_0.0, 4;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55ecb1a960a0_0, 0, 1;
    %pushi/vec4 0, 0, 26;
    %store/vec4 v0x55ecb1a4aba0_0, 0, 26;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55ecb1a4aba0_0;
    %cmpi/u 50000000, 0, 26;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_0.2, 5;
    %pushi/vec4 0, 0, 26;
    %store/vec4 v0x55ecb1a4aba0_0, 0, 26;
    %load/vec4 v0x55ecb1a960a0_0;
    %inv;
    %store/vec4 v0x55ecb1a960a0_0, 0, 1;
T_0.2 ;
    %load/vec4 v0x55ecb1a4aba0_0;
    %addi 1, 0, 26;
    %assign/vec4 v0x55ecb1a4aba0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55ecb1a83930;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55ecb1a96280_0, 0, 1;
T_1.0 ;
    %delay 20000, 0;
    %load/vec4 v0x55ecb1a96280_0;
    %inv;
    %store/vec4 v0x55ecb1a96280_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x55ecb1a83930;
T_2 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55ecb1a96420_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55ecb1a96420_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55ecb1a96420_0, 0, 1;
    %delay 1000000, 0;
    %vpi_call 2 19 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55ecb1a83930;
T_3 ;
    %vpi_call 2 21 "$monitor", $time, " inClk = %b, reset = %b, outClk = %b", v0x55ecb1a96280_0, v0x55ecb1a96420_0, v0x55ecb1a96350_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "tb_clk_divider.v";
    "clk_divider.v";
